    <script>
    //Passe Hoehe der einzelnen Felder an
    function setHeight(klasse)
    {
        var temp = 0;
        $(klasse).each(function (index) {
            if($(this).outerHeight() > temp)
            {
                temp = $(this).outerHeight();
            }          
        });

        $(klasse).css('min-height',temp);

    }
    setHeight(".list-group .list-group-item-wrapper");
    setHeight(".list-group .maschine-group-item-wrapper");
    setHeight(".list-group .streck-group-item-wrapper");
    setHeight(".list-group .trans-group-item-wrapper");
    setHeight(".list-group .handling-group-item-wrapper");
    setHeight(".header-streck");

    datetime = new Date();
    $("#datetime").html(datetime.toISOString());
    </script>
